RequestFile #?.info >ENV:WBINFO
IF NOT "$WBINFO" EQ "" >NIL:
   List $WBINFO QUICK NOHEAD LFORMAT %E TO ENV:WBEXT
   IF "$WBEXT" EQ "info"
      List $WBINFO QUICK NOHEAD LFORMAT %M TO ENV:WBRUN
      WBRun "$WBRUN" SYNC
   ELSE
      List $WBINFO QUICK NOHEAD LFORMAT %N TO ENV:WBRUN
      $WBRUN
   ENDIF
ENDIF
UNSET WBINFO
